@article{BolinNumericalsolutionfractional2020,
  title = {Numerical Solution of Fractional Elliptic Stochastic {{PDEs}} with Spatial White Noise},
  author = {Bolin, David and Kirchner, Kristin and Kovács, Mihály},
  date = {2020-04-24},
  journaltitle = {IMA Journal of Numerical Analysis},
  shortjournal = {IMA Journal of Numerical Analysis},
  volume = {40},
