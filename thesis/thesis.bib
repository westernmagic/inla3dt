  keywords = {Statistics - Applications,Statistics - Computation,Statistics - Methodology},
  file = {/home/westernmagic/Zotero/storage/2IS5UJ8A/Sidén et al_2020_Spatial 3D Mat-'ern priors for fast whole-brain fMRI analysis.pdf;/home/westernmagic/Zotero/storage/LBTDKL9G/1906.html}
